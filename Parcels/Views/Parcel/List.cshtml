<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <title>Parcel Service</title>
  </head>
  <body>
    <div class="container">
      <h1>Here are your package(s)!</h1>
      <h1>A Massive List</h1>
      <div class="row">
        <div class="col-md-2">
          <strong>Package Size</strong>
        </div>
        <div class="col-md-2">
          <strong>Package Volume</strong>
        </div>
        <div class="col-md-2">
          <strong>Cost to Ship</strong>
        </div>
      </div>
      <hr>
      @foreach( Parcel.Models.Parcel parcel in Model)
      {
        <div class="row">
          <div class="col-md-2">
            @parcel.Length X @parcel.Width X parcel.Height
          </div>
          <div class="col-md-2">
            @parcel.GetVolume();
          </div>
          <div class="col-md-2">
            $@parcel.GetCost();
          </div>
        </div>
      }
      <br>
      <a href='/cars/new'>Add a new parcel<a/>
    </div>
  </body>
  </html>
